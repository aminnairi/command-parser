{
  "name": "@aminnairi/command-parser",
  "version": "0.0.4",
  "description": "Command Line Parser",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.config.ts",
      "**/*.js",
      "**/*.spec.ts"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  },
  "scripts": {
    "test": "nyc mocha -r ts-node/register src/**/*.spec.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "development": "webpack --env.development",
    "production": "webpack --env.production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aminnairi/command-parser.git"
  },
  "keywords": [
    "cli",
    "parser",
    "command"
  ],
  "author": "Amin NAIRI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aminnairi/command-parser/issues"
  },
  "homepage": "https://github.com/aminnairi/command-parser#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/webpack": "^4.4.22",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "prettier-loader": "^2.1.1",
    "ts-loader": "^5.3.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2",
    "typescript-estree": "^8.1.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2"
  }
}
